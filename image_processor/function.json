{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "imageinput",
      "type": "blobTrigger",
      "direction": "in",
      "path": "images/raw/{name}",
      "connection": "IMAGE_STORAGE_CONNECTION_STRING"
    }
    ,{
      "name": "croppedimageoutput",
      "type": "blob",
